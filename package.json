{
  "name": "florida-graphlib",
  "version": "3.0.0",
  "description": "A directed and undirected multi-graph library",
  "scripts": {
    "test": "jest",
    "test:benchmark": "babel-node scripts/bench",
    "build": "rm -rf build; NODE_ENV=production babel src --ignore spec.js,spec.util.js --out-dir build"
  },
  "main": "index.js",
  "keywords": [
    "graph",
    "algorithms"
  ],
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "benchmark": "^1.0.0",
    "browserify": "^13.0.0",
    "chai": "^1.9.2",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "seedrandom": "^2.4.2",
    "semver": "^4.1.0",
    "sprintf": "^0.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/floridajs/graphlib.git"
  },
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
